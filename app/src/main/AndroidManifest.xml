<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hands.clean">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CleanWashHands">
        <activity
            android:name="com.hands.clean.NavigationActivity"
            android:theme="@style/Theme.CleanWashHands.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hands.clean.TutorialActivity" />
        <activity android:name="com.hands.clean.setting.GpsSettingActivity" />
        <activity android:name="com.hands.clean.setting.WifiSettingActivity" />
        <activity android:name="com.hands.clean.setting.BluetoothSettingActivity" />
    </application>

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <!-- If your app targets Android 9 or lower, you can declare ACCESS_COARSE_LOCATION instead. -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
</manifest>