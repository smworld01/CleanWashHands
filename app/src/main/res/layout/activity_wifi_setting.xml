<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hands.clean.activity.settings.wifi.WifiSettingActivity"
    android:orientation="vertical"
    android:divider="?attr/dividerHorizontal"
    android:showDividers="middle|end"
    >

    <com.hands.clean.view.CustomSwitch
        android:id="@+id/switchWifi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:contentTitle="와이파이 알림"
        app:contentContext="설명"
        />


    <com.hands.clean.view.CustomSwitch
        android:id="@+id/switchNewDeviceWifi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:contentTitle="새로운 기기 연결시 등록 알림"
        app:contentContext="설명"
        />

    <com.hands.clean.view.CustomSwitch
        android:id="@+id/switchEncryptionWifi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:contentTitle="비밀번호가 있는 와이파이 연결시 알림"
        app:contentContext="설명"
        />

    <com.hands.clean.view.CustomTextViewDescription
        android:id="@+id/buttonTrackerList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/selectableItemBackground"
        android:layout_marginStart="20dp"
        app:contentTitle="등록된 와이파이 기기 관리"
        app:contentContext="설명"
        />

    <com.hands.clean.view.CustomTextViewDescription
        android:id="@+id/buttonWifiScan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/selectableItemBackground"
        android:layout_marginStart="20dp"
        app:contentTitle="주변 와이파이 스캔해서 등록하기"
        app:contentContext="설명"
        />

</LinearLayout>